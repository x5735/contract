// File: LuncBurner.sol


pragma solidity ^0.8.0;




contract BNB_Distributor is Ownable, ReentrancyGuard {
    using SafeMath for uint256;

    address payable public luncBurnWallet;
    address payable public developmentWallet;
    address payable public marketingWallet;
    address payable public ownerWallet1;
    address payable public ownerWallet2;

    uint256 public luncBurnPercentage;
    uint256 public developmentPercentage;
    uint256 public marketingPercentage;
    uint256 public ownerWallet1Percentage;
    uint256 public ownerWallet2Percentage;

    event Distribution(uint256 amount, uint256 timeStamp);

    constructor(
        address payable _luncBurnWallet,
        address payable _developmentWallet,
        address payable _marketingWallet,
        address payable _ownerWallet1,
        address payable _ownerWallet2
    ) {
        setWalletAddresses(_luncBurnWallet, _developmentWallet, _marketingWallet, _ownerWallet1, _ownerWallet2);
        setDistributionPercentages(9800, 100, 50, 25, 25);
    }

    function setDistributionPercentages(
        uint256 _luncBurnPercentage,
        uint256 _developmentPercentage,
        uint256 _marketingPercentage,
        uint256 _ownerWallet1Percentage,
        uint256 _ownerWallet2Percentage
    ) public onlyOwner {

        require(_luncBurnPercentage <= 10000, "Invalid luncBurnPercentage");
        require(_developmentPercentage <= 10000, "Invalid developmentPercentage");
        require(_marketingPercentage <= 10000, "Invalid marketingPercentage");
        require(_ownerWallet1Percentage <= 10000, "Invalid ownerWallet1Percentage");
        require(_ownerWallet2Percentage <= 10000, "Invalid ownerWallet2Percentage");

        uint256 total = _luncBurnPercentage
            .add(_developmentPercentage)
            .add(_marketingPercentage)
            .add(_ownerWallet1Percentage)
            .add(_ownerWallet2Percentage);
        require(total == 10000, "Percentages must add up to 10000");

        luncBurnPercentage = _luncBurnPercentage;
        developmentPercentage = _developmentPercentage;
        marketingPercentage = _marketingPercentage;
        ownerWallet1Percentage = _ownerWallet1Percentage;
        ownerWallet2Percentage = _ownerWallet2Percentage;
    }

    function setWalletAddresses(
        address payable _luncBurnWallet,
        address payable _developmentWallet,
        address payable _marketingWallet,
        address payable _ownerWallet1,
        address payable _ownerWallet2
    ) public onlyOwner {
        require(_luncBurnWallet != address(0), "Invalid luncBurnWallet address");
        require(_developmentWallet != address(0), "Invalid developmentWallet address");
        require(_marketingWallet != address(0), "Invalid marketingWallet address");
        require(_ownerWallet1 != address(0), "Invalid ownerWallet1 address");
        require(_ownerWallet2 != address(0), "Invalid ownerWallet2 address");

        luncBurnWallet = _luncBurnWallet;
        developmentWallet = _developmentWallet;
        marketingWallet = _marketingWallet;
        ownerWallet1 = _ownerWallet1;
        ownerWallet2 = _ownerWallet2;
    }

    function safeTransfer(address payable to, uint256 amount) internal {
    (bool success, ) = to.call{value: amount}("");
    require(success, "Transfer failed");
    }



    receive() external payable nonReentrant {
        uint256 amount = msg.value;
        require(amount > 0, "No BNB received");
        

        uint256 luncBurnAmount = amount.mul(luncBurnPercentage).div(10000);
        uint256 developmentAmount = amount.mul(developmentPercentage).div(10000);
        uint256 marketingAmount = amount.mul(marketingPercentage).div(10000);
        uint256 ownerWallet1Amount = amount.mul(ownerWallet1Percentage).div(10000);
        uint256 ownerWallet2Amount = amount.mul(ownerWallet2Percentage).div(10000);
        
        safeTransfer(luncBurnWallet, luncBurnAmount);
        safeTransfer(developmentWallet, developmentAmount);
        safeTransfer(marketingWallet, marketingAmount);
        safeTransfer(ownerWallet1, ownerWallet1Amount);
        safeTransfer(ownerWallet2, ownerWallet2Amount);


        emit Distribution(amount, block.timestamp);
    }

    function withdrawLeftover() external onlyOwner {
    require(address(this).balance > 0, "No BNB left in the contract");
    safeTransfer(payable(owner()), address(this).balance);
    }

}