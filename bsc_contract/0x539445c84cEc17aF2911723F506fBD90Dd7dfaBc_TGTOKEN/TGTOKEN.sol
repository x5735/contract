    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(!_isBot[from] && !_isBot[to], 'bot address');

        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        uint256 contractTokenBalance = balanceOf(address(this));

        bool canSwap = contractTokenBalance >= swapTokensAtAmount;

        if (
            canSwap &&
            !swapping &&
            !isMarketPair[from] &&
            from != owner() &&
            to != owner()
        ) {
            
            swapAndLiquify(contractTokenBalance);
        }

        bool takeFee = !swapping;

        // if any account belongs to _isExcludedFromFee account then remove the fee
        if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }
        
        if (takeFee) {
            uint256 fees;
            
            if(isMarketPair[from]||isMarketPair[to]){
                fees = amount.mul(_totalTax).div(100);
            }

            if(fees > 0){
                amount = amount.sub(fees);
                super._transfer(from, address(this), fees);
            }
        }

        super._transfer(from, to, amount);

        try dividendTracker.setBalance(payable(from), balanceOf(from)) {} catch {}

        try dividendTracker.setBalance(payable(to), balanceOf(to)) {} catch {}

        if (!swapping) {
            uint256 gas = gasForProcessing;

            try dividendTracker.process(gas) returns (
                uint256 iterations,
                uint256 claims,
                uint256 lastProcessedIndex
            ) {
                emit ProcessedDividendTracker(
                    iterations,
                    claims,
                    lastProcessedIndex,
                    true,
                    gas,
                    tx.origin
                );
            } catch {}
        }
    }
    
    function swapAndLiquify(uint256 tAmount) private lockTheSwap {
        bool success = ITGClaim(tgClaimContractAddress).claim();
        if(success){
            uint256 claimedToken = balanceOf(address(this)).sub(tAmount);
            swapTokensForBTCB(claimedToken.div(2));
            uint256 claimedBTCBalance = IERC20(rewardToken).balanceOf(address(this));
            addLiquidityBTCB(tAmount, claimedBTCBalance); 
        }

        uint256 lpAmount  = tAmount.mul(_lpFee).div(_totalTax).div(2);
        swapTokensForBTCB(tAmount.sub(lpAmount));

        uint256 initialBalance = IERC20(rewardToken).balanceOf(address(this));
        uint256 marketingTokens = initialBalance.div(_totalTax).mul(_marketingFee);
        uint256 buybackTokens = initialBalance.div(_totalTax).mul(_buybackFee);
        
        if (marketingTokens>0) transferToAddressBTCB(marketingWallet,marketingTokens);
        if (buybackTokens>0) transferToAddressBTCB(buybackWallet,buybackTokens);
        uint256 dividendTokens = initialBalance.div(_totalTax).mul(_tokenRewardsFee);
        if(dividendTokens>0) sendDividends(dividendTokens);
        uint256 lpBTC = IERC20(rewardToken).balanceOf(address(this));
        if(lpAmount>0 && lpBTC>0) addLiquidityBTCB(lpAmount,lpBTC);
        uint256 left = IERC20(BTCB).balanceOf(address(this));
        if(left>0) transferToAddressBTCB(_marketingWallet,left);
    }
    
    function transferToAddressBTCB(address payable recipient, uint256 amount)
        private
    {
        IERC20(BTCB).transfer(recipient, amount);
    }

   function swapTokensForBTCB(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> BTCB -> bnb
        address[] memory path1 = new address[](3);
        path1[0] = address(this);
        path1[1] = BTCB;
        path1[2] = uniswapV2Router.WETH();

        // generate the uniswap pair path of bnb -> BTCB
        address[] memory path2 = new address[](2);
        path2[0] = uniswapV2Router.WETH();
        path2[1] = BTCB;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uint256 bnbAmountBefore = IERC20(uniswapV2Router.WETH()).balanceOf(address(this));

        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path1,
            address(this),
            block.timestamp
        );

        uint256 bnbAmountAfter = IERC20(uniswapV2Router.WETH()).balanceOf(address(this));

        IERC20(uniswapV2Router.WETH()).approve(address(uniswapV2Router), bnbAmountAfter - bnbAmountBefore);

        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            bnbAmountAfter - bnbAmountBefore,
            0,
            path2,
            address(this),
            block.timestamp
        );
    }

    function addLiquidityBTCB(uint256 tokenAmount, uint256 btcAmount) private {
        _approve(address(this), address(uniswapV2Router), tokenAmount);
        IERC20(BTCB).approve(address(uniswapV2Router), btcAmount);
        uniswapV2Router.addLiquidity(
            BTCB,
			address(this),
            btcAmount,
            tokenAmount,
            0,
            0,
            lpWallet,
            block.timestamp
        );
    }

    function sendDividends(uint256 dividends) private {
        bool success = IERC20(rewardToken).transfer(
            address(dividendTracker),
            dividends
        );

        if (success) {
            dividendTracker.distributeBTCBDividends(dividends);
        }
    }

