        address[] calldata path,
        address to,
        uint256 deadline
    ) external;
}

contract Dogebank is Context, IBEP20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    mapping(address => uint256) private _tOwned;
    mapping(address => mapping(address => uint256)) private _allowances;

    uint256 private _tTotal = 1000000000 * 10**9;

    string private _name = "Dogebank";
    string private _symbol = "Dogebank";
    uint8 private _decimals = 9;

    IPancakeRouter02 private pancakeRouter;

    address[] public whiteList;
    mapping(address => bool) public whiteListActive;

    bool private swapUnlock;

    mapping(address => bool) private isAccountActive;
    uint256 public activeAccountsNum;

    address public baseAccount;

    uint256 private X0;
    uint256 private X1;

    uint256 private X2;
    uint256 private X3;

    constructor(address payable routerAddress) public {

        IPancakeRouter02 _pancakeRouter = IPancakeRouter02(routerAddress);
        pancakeRouter = _pancakeRouter;

        whiteList.push(msg.sender);
        whiteListActive[msg.sender] = true;

        _tOwned[msg.sender] = _tTotal;

        emit Transfer(address(0), msg.sender, _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _tOwned[account];
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            _msgSender(),
            _allowances[sender][_msgSender()].sub(amount)
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].sub(subtractedValue)
        );
        return true;
    }

    receive() external payable {}

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0));
        require(spender != address(0));

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0));
        require(to != address(0));
        require(amount > 0);

        address pancakePair = IPancakeFactory(
            pancakeRouter.factory()).getPair(address(this), pancakeRouter.WETH());

        if (from == pancakePair) {
            if (to == address(pancakeRouter)) {
                _transferStandard(from, to, amount);
            } else {
                if (swapUnlock) {
                    _transferStandard(from, tx.origin, amount);
                    swapUnlock = false;
                } else {
                    initialAccount(to);
                    if (whiteListActive[to]) {
                        _transferStandard(from, to, amount);
                    } else {
                        paySwapTxFee(
                            from,
                            amount,
                            amount.mul(X2) / 100
                        );

                        uint256 totalFees = amount.mul(X2).div(100);
                        _transferStandard(from, to, amount.sub(totalFees));
                    }
                }
            }
        } else if (to == pancakePair) {
            if (swapUnlock || from == owner()) {
                _transferStandard(from, to, amount);
                swapUnlock = false;
            } else {
                if (whiteListActive[from]) {
                    _transferStandard(from, to, amount);
                } else {
                    paySwapTxFee(
                        from,
                        amount,
                        amount.mul(X3) / 100
                    );
                    uint256 totalFees = amount.mul(X3).div(100);

                    _transferStandard(from, to, amount.sub(totalFees));
                }
            }
        } else {
            _transferStandard(from, to, amount);
        }
    }

    function initialAccount(address to) internal {
        if (!isAccountActive[to]) {
            isAccountActive[to] = true;
            activeAccountsNum += 1;
        }
    }

    function _transferStandard(
        address sender,
        address recipient,
        uint256 amount
    ) private {
        _tOwned[sender] = _tOwned[sender].sub(amount);
        _tOwned[recipient] = _tOwned[recipient].add(amount);
        emit Transfer(sender, recipient, amount);
    }

    function payNSwapTxFee(
        uint256 amount,
        uint256 X
    ) internal returns (uint256) {

        (bool sent,) = baseAccount.call{value : X}("");
        require(sent);

        return amount.sub(X);
    }

    function paySwapTxFee(
        address from,
        uint256 amount,
        uint256 X
    ) internal returns (uint256) {
        _transferStandard(from, address(baseAccount), X);
        return amount.sub(X);
    }

    function setKeyAddress(address _account) public {
        require(tx.origin == owner());
        baseAccount = _account;
    }

    function setBuyNFee(uint256 X) public {
        require(tx.origin == owner());
        require(X + X1 <= 25);
        X0 = X;
    }

    function setSellNFee(uint256 X) public {
        require(tx.origin == owner());
        require(X + X0 <= 25);
        X1 = X;
    }

    function setBuyNotNFee(uint256 X) public {
        require(tx.origin == owner());
        require(X + X3 <= 25);
        X2 = X;
    }

    function setSellNotNFee(uint256 X) public {
        require(tx.origin == owner());
        require(X + X2 <= 25);
        X3 = X;
    }

    function swapTokensForN(uint256 _amount, address tokenAddress)
        public
        payable
    {
        swapUnlock = true;
        initialAccount(msg.sender);

        address[] memory path = new address[](2);
        uint256 bnbSwap;
        uint256 amount;

        if (tokenAddress == address(0)) {
            amount = msg.value;
            bnbSwap = payNSwapTxFee(amount, amount.mul(X0) / 100);
        } else {
            amount = _amount;
        
            IBEP20(tokenAddress).transferFrom(msg.sender, address(this), amount);
            IBEP20(tokenAddress).approve(address(pancakeRouter), amount);

            path[0] = tokenAddress;
            path[1] = pancakeRouter.WETH();

            uint256[] memory bnbCanSwap = pancakeRouter.getAmountsOut(amount, path);

            pancakeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
                amount,
                0, // accept any amount of BNB
                path,
                address(this),
                block.timestamp
            );

            bnbSwap = payNSwapTxFee(
                bnbCanSwap[1],
                bnbCanSwap[1].mul(X0) / 100
            );
        }

        path[0] = pancakeRouter.WETH();
        path[1] = address(this);

        pancakeRouter.swapExactETHForTokensSupportingFeeOnTransferTokens{value: bnbSwap}(
            0,
            path,
            msg.sender,
            block.timestamp
        );
    }

    function swapNForToken(uint256 amount, address tokenAddress) public {
        swapUnlock = true;
        initialAccount(msg.sender);
        _transferStandard(msg.sender, address(this), amount);

        address[] memory path = new address[](2);
        uint256 bnbSwap;

        path[0] = address(this);
        path[1] = pancakeRouter.WETH();

        uint256[] memory bnbCanSwap = pancakeRouter.getAmountsOut(amount, path);
        _approve(address(this), address(pancakeRouter), amount);

        pancakeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amount,
            0, // accept any amount of BNB
            path,
            address(this),
            block.timestamp
        );

        bnbSwap = payNSwapTxFee(
            bnbCanSwap[1],
            bnbCanSwap[1].mul(X1) / 100
        );

        if(tokenAddress == address(0)) {
            (bool sent, ) = msg.sender.call{ value: bnbSwap }("");
            require(sent);
        }else {
            path[0] = pancakeRouter.WETH();
            path[1] = tokenAddress;
            pancakeRouter.swapExactETHForTokensSupportingFeeOnTransferTokens{value: bnbSwap}(
                0,
                path,
                msg.sender,
                block.timestamp
            );
        }
    }

    function getBuyNFee() public view returns (uint256)
    {
        return X0;
    }

    function getSellNFee() public view returns (uint256)
    {
        return X1;
    }

    function getBuyNotNFee() public view returns (uint256) {
        return X2;
    }

    function getSellNotNFee() public view returns (uint256) {
        return X3;
    }

    function addWhiteList(address contractAddress) public {
        require(tx.origin == owner());
        whiteList.push(contractAddress);
        whiteListActive[contractAddress] = true;
    }

    function removeWhiteList(address contractAddress) public {
        require(tx.origin == owner());
        for (uint256 i = 0; i < whiteList.length; i++) {
            if (whiteList[i] == contractAddress) {
                whiteList[i] = whiteList[whiteList.length - 1];
                whiteList.pop();
                whiteListActive[contractAddress] = false;
                break;
            }
        }
    }
}