        uint256 tDevelopment;
        uint256 tBurn;
        uint256 tTransferAmount;
    }

    function _getTValues(
        uint256 tAmount
    ) private returns (uint256, uint256, uint256) {
        TValues memory tValues;
        tValues.tDevelopment = calculateDevelopmentFee(tAmount);
        tValues.tBurn = calculateBurnFee(tAmount);

        _totalBurnt += tValues.tBurn;
        _totalDevelopment += tValues.tDevelopment;

        if (_totalBurnt > _burnLimit) {
            tValues.tBurn -= (_totalBurnt - _burnLimit);

          

            uint256 devAftrBurnLmtCut = (_charityFee * tValues.tBurn).div(
                1000
            );
            _charityFee += _burnFee;
            tValues.tDevelopment =
                calculateDevelopmentFee(tAmount.sub(tValues.tBurn.mul(100))) +
                devAftrBurnLmtCut;

            _burnFee = 0;
            _totalBurnt = _burnLimit;
        }
        tValues.tTransferAmount = tAmount
            .sub(tValues.tDevelopment)
            .sub(tValues.tBurn);
        return (
            tValues.tTransferAmount,
            tValues.tDevelopment,
            tValues.tBurn
        );
    }

  

