        _charitytWalletAddress = devWallet;
    }

    function setStakingContract(address stakingContract) external onlyOwner {
        _stakingContractAddress = stakingContract;
    }

    receive() external payable {}

    struct FeesInfo {
        uint256 tTransferAmount;
        uint256 tDevelopment;
        uint256 tBurn;
    }

    function _getValues(
        uint256 tAmount
    ) private returns (uint256, uint256, uint256) {
        FeesInfo memory feesInfo;
        (
            feesInfo.tTransferAmount,
            feesInfo.tDevelopment,
            feesInfo.tBurn
        ) = _getTValues(tAmount);
        return (
            feesInfo.tTransferAmount,
            feesInfo.tDevelopment,
            feesInfo.tBurn
        );
    }

